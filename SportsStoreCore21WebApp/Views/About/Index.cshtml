@model IDictionary<string, string>
@{
  ViewData["Title"] = "Index";
}
<h1>About Information</h1>
<table class="table table-bordered table-responsive-sm table-hover table-striped col-6">
  <tr>
    <th>Id</th>
    <th>Value</th>
  </tr>
  @foreach (var item in Model)
  {
    <tr>
      <td>@item.Key</td>
      <td>@item.Value</td>
    </tr>
  }
</table>





